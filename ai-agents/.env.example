# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1
GOOGLE_CLOUD_ZONE=us-central1-a

# Gemini API Configuration
GEMINI_API_KEY=your-gemini-api-key
GEMINI_PROJECT_ID=your-project-id

# Kubernetes Configuration
CLUSTER_NAME=ai-boutique-cluster
KUBECTL_AI_ENABLED=true

# ADK Configuration
ADK_RUNTIME_MODE=development
ADK_HOT_RELOAD=true
ADK_LOG_LEVEL=debug

# MCP Server Configuration
MCP_BOUTIQUE_API_PORT=8080
MCP_ANALYTICS_PORT=8081
MCP_ML_MODELS_PORT=8082

# A2A Protocol Configuration
A2A_PROTOCOL_PORT=9090
A2A_DISCOVERY_ENABLED=true

# Development Configuration
NODE_ENV=development
LOG_LEVEL=debug
HOT_RELOAD_ENABLED=true

# Online Boutique Integration
BOUTIQUE_FRONTEND_URL=http://frontend:80
BOUTIQUE_API_GATEWAY=http://api-gateway:8080

# Security Configuration
JWT_SECRET=your-jwt-secret-key
CORS_ORIGIN=http://localhost:3000

# Performance Configuration
CACHE_TTL=300
MAX_CONCURRENT_REQUESTS=100
REQUEST_TIMEOUT=30000